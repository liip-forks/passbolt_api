<% if(this.resources) { %>
<%= __('This user is the sole owner of %s %s', resources.length, resources.length > 1 ? __('passwords') : __('password')) %>:
<% for(var i in resources) { %><%= (i != 0 ? ', ' : '') + resources[i].Resource.name %><% } %>.
<br/>
<% } %>

<% if(this.groups) { %>
<%= __('This user is the sole manager of %s %s',  groups.length, groups.length > 1 ? __('groups') : __('group')) %>:
<% for(var i in groups) { %><%= (i != 0 ? ', ' : '') + groups[i].Group.name %><% } %>.
<br/>
<% } %>

<br/>

<%= __('You need to transfer the ownership to other users before you can proceed.') %>
