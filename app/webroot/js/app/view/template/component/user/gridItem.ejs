<tr id="<%= id %>" class="<%= (item.active == '0' ? 'inactive' : '') %>">

    <% for (var i in columnModels) { %>

    <td class="js_grid_column_<%= columnModels[i].name %> cell_<%= columnModels[i].name %> <%= columnModels[i].css.join(' ') %>">
        <div title="<%== titles[columnModels[i].name] %>">
            <%== values[columnModels[i].name] %>
        </div>
    </td>

    <% } %>

</tr>
