{
  "name": "Passbolt/DirectorySync",
  "description": "Passbolt/DirectorySync plugin for CakePHP",
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "cakephp/cakephp": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Passbolt\\DirectorySync\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Passbolt\\DirectorySync\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
